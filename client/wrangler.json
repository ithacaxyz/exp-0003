{
  "$schema": "https://esm.sh/wrangler/config-schema.json",
  "minify": true,
  "keep_vars": true,
  "workers_dev": true,
  "name": "exp-0003-client",
  "compatibility_date": "2025-05-27",
  "compatibility_flags": [
    "nodejs_als",
    "nodejs_compat"
  ],
  "dev": {
    "port": 6901
  },
  "placement": {
    "mode": "smart"
  },
  "observability": {
    "enabled": true
  },
  "assets": {
    "directory": "./dist"
  },
  "vars": {
    "VITE_DISABLE_DIALOG": true
  }
}
